{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
    "fmt": "next lint --dir . --fix; prettier --write .",
    "pregenerate:client:graphql": "rimraf lib/generated/graphql",
    "generate:client:graphql": "graphql-codegen --config codegen.ts",
    "postgenerate:client:graphql": "next lint --dir lib/generated/graphql --fix && prettier --write lib/generated/graphql",
    "pregenerate:client:openapi": "rimraf lib/generated/openapi",
    "generate:client:openapi": "openapi2aspida --input=${FRONTEND_BFF_ENDPOINT_OPENAPI}/swagger-json --outputdir=lib/generated/openapi",
    "postgenerate:client:openapi": "next lint --dir lib/generated/reopenapist --fix && prettier --write lib/generated/openapi",
    "generate:clients": "concurrently \"npm:generate:client:*\"",
    "lint": "next lint --dir .; prettier --check .",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --ci",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch"
  },
  "dependencies": {
    "@aspida/fetch": "^1.14.0",
    "@aspida/swr": "^1.14.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "http-proxy": "^1.18.1",
    "next": "15.5.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/builder-webpack5": "^9.1.3",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^9.1.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/babel__core": "^7.20.5",
    "@types/eslint": "9.6.1",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "24.3.0",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.8",
    "@types/rimraf": "^4.0.5",
    "@types/testing-library__jest-dom": "^6.0.0",
    "babel-loader": "^10.0.0",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^9.1.3",
    "graphql-codegen-plugin-typescript-swr": "^0.8.3",
    "jest": "^30.1.0",
    "jest-environment-jsdom": "^30.1.0",
    "openapi2aspida": "^0.19.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
