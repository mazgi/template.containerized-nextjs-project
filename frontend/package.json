{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
    "fmt": "next lint --dir . --fix; prettier --write .",
    "pregenerate:client:graphql": "rimraf lib/generated/graphql",
    "generate:client:graphql": "graphql-codegen --config codegen.ts",
    "postgenerate:client:graphql": "next lint --dir lib/generated/graphql --fix && prettier --write lib/generated/graphql",
    "pregenerate:client:openapi": "rimraf lib/generated/openapi",
    "generate:client:openapi": "openapi2aspida --input=${FRONTEND_BFF_ENDPOINT_OPENAPI}/swagger-json --outputdir=lib/generated/openapi",
    "postgenerate:client:openapi": "next lint --dir lib/generated/reopenapist --fix && prettier --write lib/generated/openapi",
    "generate:clients": "concurrently \"npm:generate:client:*\"",
    "lint": "next lint --dir .; prettier --check .",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --ci",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch"
  },
  "dependencies": {
    "@aspida/fetch": "^1.11.0",
    "@aspida/swr": "^1.11.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "http-proxy": "^1.18.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/client-preset": "^1.2.4",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "^2.8.6",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.11",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/http-proxy": "^1.17.9",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "babel-loader": "^9.1.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.8",
    "graphql-codegen-plugin-typescript-swr": "^0.8.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "openapi2aspida": "^0.19.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  }
}
