{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
    "fmt": "next lint --dir . --fix; prettier --write .",
    "pregenerate:client:graphql": "rimraf lib/generated/graphql",
    "generate:client:graphql": "graphql-codegen --config codegen.ts",
    "postgenerate:client:graphql": "next lint --dir lib/generated/graphql --fix && prettier --write lib/generated/graphql",
    "pregenerate:client:openapi": "rimraf lib/generated/openapi",
    "generate:client:openapi": "openapi2aspida --input=${FRONTEND_BFF_ENDPOINT_OPENAPI}/swagger-json --outputdir=lib/generated/openapi",
    "postgenerate:client:openapi": "next lint --dir lib/generated/reopenapist --fix && prettier --write lib/generated/openapi",
    "generate:clients": "concurrently \"npm:generate:client:*\"",
    "lint": "next lint --dir .; prettier --check .",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --ci",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch"
  },
  "dependencies": {
    "@aspida/fetch": "^1.12.0",
    "@aspida/swr": "^1.12.0",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "http-proxy": "^1.18.1",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/client-preset": "^3.0.0",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "^3.0.3",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/builder-webpack5": "^7.0.5",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.0.5",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/babel__core": "^7.20.0",
    "@types/eslint": "8.37.0",
    "@types/http-proxy": "^1.17.10",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/rimraf": "^3.0.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "babel-loader": "^9.1.2",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "graphql-codegen-plugin-typescript-swr": "^0.8.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "openapi2aspida": "^0.19.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
